fwords = open("sentiment_words.txt", "r")
fpos = open("positive.txt", "w")
fneg = open("positive.txt", "w")
fextrafood = open("positive.txt", "w")

for line in fwords:
    line = line.strip()
    if line[0] == '.':
        fpos.write(line[1:])
    elif line[0] == '!':
        fneg.write(line[1:])
    elif line[0] == '*':
        fwords.write(line[1:])

close(fwords)
close(fpos)
close(fneg)
close(fextrafood)
